{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bypass",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/progress",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/webhook",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
